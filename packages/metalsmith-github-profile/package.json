{
  "name": "metalsmith-github-profile",
  "version": "2.0.3",
  "description": "A Metalsmith plugin to fetch GitHub profile information. ",
  "keywords": [
    "metalsmith",
    "metalsmith-plugin",
    "plugin"
  ],
  "homepage": "https://github.com/emmercm/metalsmith-plugins/tree/main/packages/metalsmith-github-profile#readme",
  "bugs": {
    "url": "https://github.com/emmercm/metalsmith-plugins/issues"
  },
  "license": "GPL-3.0-or-later",
  "author": "Christian Emmer",
  "type": "module",
  "files": [
    "dist",
    "!**/jest.*",
    "!**/test/**",
    "!**/*.test.*",
    "!**/*.tsbuildinfo"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmercm/metalsmith-plugins.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --verbose && eslint .",
    "test:update": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --coverage --updateSnapshot",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --verbose --coverage",
    "prepack": "npm run build && cp -f ../../LICENSE ./",
    "postpack": "rm ./LICENSE"
  },
  "dependencies": {
    "async": "^3.2.6",
    "deepmerge": "^4.3.1"
  },
  "peerDependencies": {
    "metalsmith": "^2.5.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.27",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "assert-dir-equal": "1.1.0",
    "eslint": "9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "handlebars": "4.7.8",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "metalsmith": "^2.6.3",
    "metalsmith-hbt-md": "1.0.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
